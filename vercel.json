{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/index.js"
        }
    ],
    "env": {
        "GEMINI_MODEL": "gemini-3-flash-preview"
    }
}