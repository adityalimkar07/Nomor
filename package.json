{
  "name": "life-simulator",
  "version": "1.0.0",
  "private": true,
  "description": "Life simulator that manages schedule, coins, and app launching.",
  "author": "Your Name",
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "backend": "node backend/index.js",
    "dev": "concurrently \"npm run backend\" \"cross-env PORT=3333 npm start\"",
    "electron": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-fetch": "^2.7.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "electron": "^31.0.2",
    "electron-builder": "^26.0.12",
    "eslint-plugin-react-hooks": "^5.2.0",
    "wait-on": "^7.2.0",
    "web-vitals": "^5.1.0"
  },
  "homepage": ".",
  "build": {
    "extends": null,
    "appId": "com.yourname.lifesimulator",
    "productName": "Life Simulator",
    "forceCodeSigning": false,
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}